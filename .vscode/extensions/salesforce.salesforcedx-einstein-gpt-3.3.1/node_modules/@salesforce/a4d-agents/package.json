{
  "name": "@salesforce/a4d-agents",
  "version": "1.6.0",
  "description": "Agents for Agentforce for Developers",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": "Salesforce",
  "homepage": "https://developer.salesforce.com/docs/platform/einstein-for-devs/overview",
  "bugs": {
    "url": "https://github.com/forcedotcom/Einstein-GPT-for-Developers/issues"
  },
  "type": "module",
  "main": "./dist/index.bundle.min.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.bundle.min.cjs"
    }
  },
  "files": [
    "dist/index.d.ts",
    "dist/index.bundle.min.cjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn build:ts && yarn build:bundle",
    "build:ts": "tsc --build tsconfig.json",
    "build:bundle": "yarn clean && node ./scripts/bundle-build.js && yarn build:bundle:dts && yarn bundle:check",
    "build:bundle:dts": "yarn dts-bundle-generator -o dist/index.d.ts ./src/index.ts --no-check --export-referenced-types=false --external-inlines=@langchain/core @lwc/signals langsmith zod @sfdc-internal/lgpt-core",
    "bundle:analyze": "yarn esbuild-visualizer --metadata dist/metadata.json --filename dist/bundle-stats.html; open dist/bundle-stats.html",
    "bundle:check": "./scripts/bundle-smoke-test.js && node ./scripts/bundle-size-check.js",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .js,.ts",
    "eslint:fix": "yarn eslint . --ext .js,.ts --fix",
    "eslint:ci": "yarn eslint . --ext .js,.ts -f json -o eslint/eslint.json",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.0",
    "@commitlint/config-conventional": "^19.7.1",
    "@salesforce/prettier-config": "0.0.3",
    "@sfdc-internal/lgpt-core": "^2.6.5",
    "@sfdc-internal/lgpt-experts-lwc": "^1.46.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "^0.24.2",
    "esbuild-visualizer": "^0.7.0",
    "eslint": "^8.57.1",
    "eslint-config-salesforce": "^2.2.0",
    "eslint-config-salesforce-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-tsdoc": "^0.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "3.4.2",
    "prettier-plugin-packagejson": "2.5.8",
    "semantic-release": "^24.2.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "zod": "^3.22.4"
  },
  "prettier": "@salesforce/prettier-config",
  "engines": {
    "node": "^20 || ^22"
  },
  "packageManager": "yarn@4.7.0"
}
